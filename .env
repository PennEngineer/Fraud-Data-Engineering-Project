# Core Configuration
VERSION=0.50.33
WORKSPACE_ROOT=/var/lib/airbyte/workspace
DATA_ROOT=/var/lib/airbyte/data
CONFIG_ROOT=/var/lib/airbyte/config
LOCAL_ROOT=/var/lib/airbyte/local
WORKSPACE_DOCKER_MOUNT=airbyte_workspace
DATA_DOCKER_MOUNT=airbyte_data  
DB_DOCKER_MOUNT=airbyte_db

# Airbyte Configuration  
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.
PYTHONPATH=/opt/airflow/include

# Snowflake Configuration (UPDATE THESE WITH YOUR ACTUAL VALUES)
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_ACCOUNT=your_account

# Database Configuration
DATABASE_USER=airbyte
DATABASE_PASSWORD=password
DATABASE_HOST=db
DATABASE_PORT=5432
DATABASE_URL=jdbc:postgresql://db:5432/airbyte

# Airbyte Configuration
DEPLOYMENT_MODE=OSS
LOG_LEVEL=INFO
HACK_LOCAL_ROOT_PARENT=/tmp
LOCAL_DOCKER_MOUNT=/var/lib/airbyte/local
LOCAL_CONNECTOR_CATALOG_PATH=/var/lib/airbyte/local

# Temporal Configuration
TEMPORAL_HOST=airbyte-temporal:7233
TEMPORAL_HISTORY_RETENTION_IN_DAYS=30

# Worker Configuration
MAX_SYNC_WORKERS=5
MAX_SPEC_WORKERS=5
MAX_CHECK_WORKERS=5
MAX_DISCOVER_WORKERS=5
MAX_NOTIFY_WORKERS=5

# Job Configuration
SYNC_JOB_MAX_ATTEMPTS=3
SYNC_JOB_MAX_TIMEOUT_DAYS=3
SYNC_JOB_INIT_RETRY_TIMEOUT_MINUTES=5
JOB_MAIN_CONTAINER_CPU_REQUEST=
JOB_MAIN_CONTAINER_CPU_LIMIT=
JOB_MAIN_CONTAINER_MEMORY_REQUEST=
JOB_MAIN_CONTAINER_MEMORY_LIMIT=
NORMALIZATION_JOB_MAIN_CONTAINER_CPU_REQUEST=
NORMALIZATION_JOB_MAIN_CONTAINER_CPU_LIMIT=
NORMALIZATION_JOB_MAIN_CONTAINER_MEMORY_REQUEST=
NORMALIZATION_JOB_MAIN_CONTAINER_MEMORY_LIMIT=

# Feature Flags
AUTO_DETECT_SCHEMA=true
APPLY_FIELD_SELECTION=false
FIELD_SELECTION_WORKSPACES=
AUTO_DISABLE_FAILING_CONNECTIONS=false
FEATURE_FLAG_CLIENT=
LAUNCHDARKLY_KEY=
SHOULD_RUN_NOTIFY_WORKFLOWS=false

# Monitoring and Metrics
TRACKING_STRATEGY=logging
PUBLISH_METRICS=false
METRIC_CLIENT=
MICROMETER_METRICS_ENABLED=false
MICROMETER_METRICS_STATSD_FLAVOR=datadog
STATSD_HOST=localhost
STATSD_PORT=8125
DD_AGENT_HOST=
DD_DOGSTATSD_PORT=8125

# Security
SECRET_PERSISTENCE=TESTING_CONFIG_DB_TABLE
SEGMENT_WRITE_KEY=
JOB_ERROR_REPORTING_STRATEGY=logging
JOB_ERROR_REPORTING_SENTRY_DSN=
OTEL_COLLECTOR_ENDPOINT=

# Web Configuration
INTERNAL_API_HOST=airbyte-server:8001
INTERNAL_API_URL=http://airbyte-server:8001/api/v1/
CONNECTOR_BUILDER_API_HOST=airbyte-connector-builder-server:80
WEBAPP_URL=http://localhost:8000/
AIRBYTE_API_HOST=localhost:8001
WORKLOAD_API_URL=

# Worker Settings
WORKER_ENVIRONMENT=docker
WORKER_LOGS_STORAGE_TYPE=local
WORKER_STATE_STORAGE_TYPE=local
LOG_CONNECTOR_MESSAGES=false
WORKERS_MICRONAUT_ENVIRONMENTS=control-plane
CRON_MICRONAUT_ENVIRONMENTS=control-plane

# Scheduler Settings
NEW_SCHEDULER=false
WORKFLOW_FAILURE_RESTART_DELAY_SECONDS=600
ACTIVITY_MAX_ATTEMPT=3
ACTIVITY_INITIAL_DELAY_BETWEEN_ATTEMPTS_SECONDS=30
ACTIVITY_MAX_DELAY_BETWEEN_ATTEMPTS_SECONDS=600

# Connector Development Kit
CDK_VERSION=
PYTHON_VERSION=3.9

# Github (for custom connectors)
GITHUB_STORE_BRANCH=master

# Basic Auth Proxy
BASIC_AUTH_USERNAME=
BASIC_AUTH_PASSWORD=
BASIC_AUTH_PROXY_TIMEOUT=900

# Soda Configuration  
CONFIGS_DATABASE_MINIMUM_FLYWAY_MIGRATION_VERSION=0.35.15.001
JOBS_DATABASE_MINIMUM_FLYWAY_MIGRATION_VERSION=0.35.15.001
UPDATE_DEFINITIONS_CRON_ENABLED=false

# Additional PIP requirements (if any)
_PIP_ADDITIONAL_REQUIREMENTS=

# Connector Registry Configuration
# Skip connector registry seeding entirely
CONNECTOR_REGISTRY_SEED_PROVIDER=
LOCAL_CONNECTOR_CATALOG_PATH=
